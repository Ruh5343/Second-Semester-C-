#include <iostream>
#include <string>
using namespace std;

int main() {
    string s;
    getline(cin, s);

    // Print first character if it's not a space
    if(s[0] != ' ')
        cout << s[0];

    // Check for each word starting after a space
    for(int i = 1; i < s.length(); i++){
        if(s[i-1] == ' ' && s[i] != ' ')
            cout << s[i];
    }

    return 0;
}
