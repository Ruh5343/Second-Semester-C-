#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    long long f1 = 0, f2 = 1, f3;
    long long sum = 0;

    for (int i = 0; i < n; i++) {
        if (i % 2 == 0) {
            sum += f1;
        }

        f3 = f1 + f2;
        f1 = f2;
        f2 = f3;
    }

    cout << "Sum = " << sum;

    return 0;
}
