#include <iostream>
#include <string>
#include <cctype>
using namespace std;

int main() {
    string s;
    getline(cin, s);

    bool present[26] = {false};

    for(char c : s){
        if(isalpha(c)){
            c = tolower(c);
            present[c - 'a'] = true;
        }
    }

    // check if all 26 letters exist
    for(int i = 0; i < 26; i++){
        if(!present[i]){
            cout << "Not Pangram";
            return 0;
        }
    }

    cout << "Pangram";
    return 0;
}
