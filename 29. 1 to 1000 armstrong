#include <iostream>
#include <cmath>
using namespace std;
int main(){
int i, sum, temp, digit;

for (int i=1;i<=1000;i++){
sum=0; //taking this because, for each number the sum will start from 0
temp=i;
//so, we won't destroy the original number
while (temp>0){
    digit=temp%10;
    sum+=digit*digit*digit;
    temp=temp/10;
}
if(sum==i){
    cout<<i<<" ";
}
}
return 0;
}
//1st loop[digit=153%10=3; digit*digit*digit=3*3*3=27; sum=0+27=27;] 2nd loop [15%10=5......sum=27+125=152].....
